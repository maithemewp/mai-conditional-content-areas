{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "mai/custom-content-area",
	"version": "0.1.0",
	"title": "Mai Custom Content Area",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example static block scaffolded with Create Block tool.",
	"attributes": {
		"cca": {
			"type": "number",
			"default": 0
		}
	},
	"supports": {
		"align": false,
		"html": false
	},
	"textdomain": "mai-custom-content-areas",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
